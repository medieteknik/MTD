<% title(' – ' + t('.title')) %>
<div class="container">
  <section class="info limit-width">
    <%= content_tag :h1, t('.title')%>
    <%= content_tag :p, t('.lead'), class: 'lead' %>
  </section>

  <section class="info">
    <div class="limit-width">
      <%= content_tag :h2, t('.lectures.title') %>
      <%= content_tag :p, t('.lectures.lead'), class: 'lead' %>
      <%= content_tag :p, t('.lectures.more')%>
    </div>

    <div class="row">
      <div class="col-sm-6 col-md-3">
        <%= image_tag 'previous/lectures/per.jpg', alt: 'Per Karlsson', class: 'img-responsive' %>
        <%= content_tag :h3, 'Per Karlsson' %>
        <%= content_tag :p, t('.lectures.per') %>
      </div>
      <div class="col-sm-6 col-md-3">
        <%= image_tag 'previous/lectures/mihai.jpg', alt: 'Mihai Aldén', class: 'img-responsive' %>
        <%= content_tag :h3, 'Mihai Aldén' %>
        <%= content_tag :p, t('.lectures.mihai') %>
      </div>
       <div class="col-sm-6 col-md-3">
        <%= image_tag 'previous/lectures/nafees.jpg', alt: 'Nafees Bin Zafar', class: 'img-responsive' %>
        <%= content_tag :h3, 'Nafees Bin Zafar' %>
        <%= content_tag :p, t('.lectures.nafees') %>
      </div>
      <div class="col-sm-6 col-md-3">
        <%= image_tag 'previous/lectures/iris.jpg', alt: 'Iris Classon', class: 'img-responsive' %>
        <%= content_tag :h3, 'Iris Classon' %>
        <%= content_tag :p, t('.lectures.iris') %>
      </div>
    </div>

  </section>

  <div class="info limit-width">
    <%= content_tag :h3, t('.video') %>
    <div class="embed-responsive embed-responsive-16by9">
      <iframe src="//www.youtube.com/embed/tPc6Rm6njaA?theme=light&showinfo=0&rel=0" frameborder="0" class="embed-responsive-item" allowfullscreen></iframe>
    </div>
  </div>
